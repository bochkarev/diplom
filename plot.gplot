#!/usr/bin/env gnuplot

set terminal png size 1024,768
set xzeroaxis lt -1
set title "Cell fluorescence decline (3000nm centrosome)"
set xrange [50:950]
#set yrange [0.6:1]
set xlabel "R, nm"
set ylabel "Fluorescence"
#set mytics 4
#set mxtics 1
set grid ytics mytics
set output "plot.png"
plot "data/10.data" using 1:2 with lines title "C = 10" lw 2, \
"data/5.data" using 1:2 with lines title "C = 5" lw 2, \
"data/2.5.data" using 1:2 with lines title "C = 2.5" lw 2, \
"data/1.data" using 1:2 with lines title "C = 1" lc rgb "black" lw 2, \
"data/0.5.data" using 1:2 with lines title "C = 0.5" lw 2
